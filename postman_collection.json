    {
      "name": "3. Previsões",
      "item": [
        {
          "name": "Previsão Simples - Sucesso",
          "event": {
            "test": {
              "script": {
                "type": "text/javascript",
                "exec": [
                  "pm.test('Status code is 200', function () {",
                  "    pm.response.to.have.status(200);",
                  "});",
                  "",
                  "pm.test('Response has projections', function () {",
                  "    var jsonData = pm.response.json();",
                  "    pm.expect(jsonData).to.have.property('projections');",
                  "    pm.expect(jsonData.projections).to.be.an('array');",
                  "});",
                  "",
                  "pm.test('Response has confidence intervals', function () {",
                  "    var jsonData = pm.response.json();",
                  "    pm.expect(jsonData).to.have.property('confidence_intervals');",
                  "});",
                  "",
                  "pm.test('Probability is between 0 and 1', function () {",
                  "    var jsonData = pm.response.json();",
                  "    pm.expect(jsonData.probability_increase).to.be.within(0, 1);",
                  "});"
                ]
              }
            }
          },
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"data\": [100, 110, 105, 120, 125, 130, 128, 135, 140, 145, 142, 150],\n  \"n_projections\": 6,\n  \"method\": \"auto\",\n  \"confidence_level\": 0.95\n}"
            },
            "url": {
              "raw": "{{base_url}}/forecast/single",
              "host": ["{{base_url}}"],
              "path": ["forecast", "single"]
            },
            "description": "Previsão para uma série temporal única"
          }
